{
  "ConnectionStrings": {
    "SqlServer": "Data Source=.;Initial Catalog=ApiDb;Integrated Security=true",
    "ElmahServer": "Data Source=.;Initial Catalog=ApiDb;Integrated Security=true"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "SiteSettings": {
    "ElmahPath": "/elmah/errors",
    "JwtSettings": {
      "SecrectKey": "MySecretKeyValue",
      "Encryptkey": "16CharEncryptKey",
      "Issuer": "MyWebsite",
      "Audience": "MyWebsite",
      "NotBeforeMinuts": "0",
      "ExpirationMinuts": "60"
    },
    "IdentitySettings": {
      "PasswordRequireDigit": "true",
      "PasswordRequiredLength": "6",
      "PasswordRequireNonAlphanumic": "false",
      "PasswordRequireUppercase": "false",
      "PasswordRequireLowercase": "false",
      "RequireUniqueEmail": "true"
    }
  }
}
